html,body
	height 100%
	overflow hidden
.navbar-static-top
	margin 0
h2
	font-family 'Open Sans', 'Trebuchet MS', sans-serif
	font-weight bold
#info-modal, #info-modal-bg, #select-modal
	position absolute
	display none
#info-modal,#select-modal
	width 300px
	height 200px
	top 50%
	left 50%
	z-index 1002
	margin-top -100px
	margin-left -150px
	background #eee
	-webkit-border-radius 5px
	-moz-border-radius 5px
	border-radius 5px
	padding 5px
#info-modal h2, #select-modal h2
	font-size 20pt
#info-modal-bg
	top 0
	left 0
	width 100%
	height 100%
	z-index 1001
	background #000
	-moz-opacity 0.30
	-khtml-opacity 0.30
	opacity 0.30
	-ms-filter "progidDXImageTransform.Microsoft.Alpha"(Opacity=30)
.modal-interior
	position relative
	height 100%
.modal-close
	position absolute
	right 5px
	bottom 5px
#rooms
	height 40px
	background #ddd
	padding-left 5px
.room-tab
	background #f5f5f5
	color #666
	margin-top 10px
	float left
	cursor pointer
	-webkit-user-select none    
	-moz-user-select none
	-ms-user-select none
	-o-user-select none
	user-select none
	margin-left 15px
.room-tab:first-child
	margin-left 10px
.room-tab.active
	background #fff
	color #333
.room-tab:after
	content 'Ã—'
	padding-right 5px
	-moz-opacity 0.30
	-khtml-opacity 0.30
	opacity 0.30
	-ms-filter "progidDXImageTransform.Microsoft.Alpha"(Opacity=30)
.room-tab:hover:after
	-moz-opacity 0.80
	-khtml-opacity 0.80
	opacity 0.80
	-ms-filter "progidDXImageTransform.Microsoft.Alpha"(Opacity=80)
.room-tab div
	padding 5px
	display inline-block
.room.room-chat
	height 100%
.room-chat
	padding-left 5px
.room-users
	background #f5f5f5
	font-weight bold
	height 100%
.room-users ul 
	padding 0
#main
	height 100%
.room
	height 100%
	overflow hidden
.room-add
	background #f5f5f5
	color #666
	margin-top 10px
	padding 5px
	float left
	cursor pointer
	-webkit-user-select none
	-moz-user-select none
	-ms-user-select none
	-o-user-select none
	user-select none
	margin-left 15px
.room-add div
	font-weight bold
	-moz-opacity 0.30
	-khtml-opacity 0.30
	opacity 0.30
	-ms-filter "progidDXImageTransform.Microsoft.Alpha"(Opacity=30)
.room-add div:hover
	-moz-opacity 0.80
	-khtml-opacity 0.80
	opacity 0.80
	-ms-filter "progidDXImageTransform.Microsoft.Alpha"(Opacity=80)
#room-menu
	height 310px
#select-modal
	height 400px
	margin-top -200px
.modal-join
	position absolute
	left 5px
	bottom 5px
#loading-bg
	width 100%
	height 100%
	position absolute
	top 0
	left 0
	z-index 1004
	background #000
	-moz-opacity 0.30
	-khtml-opacity 0.30
	opacity 0.30
	-ms-filter "progidDXImageTransform.Microsoft.Alpha"(Opacity=30)
#loading-text
	text-align center
	font-weight bold
	color #666
#loading .center
	position absolute
	width 500px
	margin-left -250px
	left 50%
	top 50%
	margin-top -54px
	z-index 1005
.room-users-list
	list-style none
.room-chat
	position relative
